{
  "extends": "next/core-web-vitals",
  "plugins": ["import", "prettier"],
  "rules": {
    "no-console": 1,
    "prettier/prettier": "error",
    "import/order": [
      "error",
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index",
          "object",
          "type"
        ],
        "newlines-between": "always"
      }
    ]
  }
}
